<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sdh.qqbot.main.mapper.MessageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.sdh.qqbot.main.entity.database.Message">
        <id column="id" property="id" />
        <result column="message_type" property="messageType" />
        <result column="message_id" property="messageId" />
        <result column="time" property="time" />
        <result column="real_id" property="realId" />
        <result column="message" property="message" />
        <result column="sender_nickname" property="senderNickname" />
        <result column="sender_userid" property="senderUserid" />
        <result column="group" property="group" />
        <result column="group_id" property="groupId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, message_type, message_id, time, real_id, message, sender_nickname, sender_userid, group, group_id
    </sql>

</mapper>
